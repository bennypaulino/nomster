<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Some Details About The Place...</h1>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <br>
    <%= f.input :address %>
    <br>
    <%= f.input :description %>
    <br>
    <%= f.submit 'Create', class: "btn btn-info" %>
  <% end %>
</div>
